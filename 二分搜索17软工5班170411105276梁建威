#include<iostream>
using namespace std;
 
int a[100];
int BinarySearch(int a[],int b,int n)
{
	int left=0,right=n-1;
	while(left<=right)
	{
		int middle=(left+right)/2;
		if(a[middle]==b) 
			return middle;
		if(b<a[middle])
			right=middle-1;
		else
			left=middle+1;	
	}
	return -1;
}
int main()
{
	int n=100,c;
	for(int i=0;i<n;i++)
	{
		a[i]=i;
	}
	cout<<"要查找哪个"<<endl;
	cin>>c; 
	cout<<"在数组的第"<<BinarySearch(a,c,n)<<"个"<<endl;
	return 0;
 } 
