public class two {
public static int sort(int []array,int a,int lo,int hi){
        if(lo<=hi){
            int mid=(lo+hi)/2;
            if(a==array[mid]){
                return mid;
            }
            else if(a>array[mid]){
            	return sort(array,a,mid+1,hi);
            }else{
                return sort(array,a,lo,mid-1);
            }
        }
        System.out.println();
        return -1;
    }
public static void main(String[] args) {
	// TODO Auto-generated method stub

        }
		
		
	}
